{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "cweagans/composer-patches": "^1.7",
    "firebase/php-jwt": "^6.10",
    "google/cloud-firestore": "^1.47",
    "jackbayliss/livewire-select2": "dev-l12-compatibility",
    "kreait/laravel-firebase": "^6.0",
    "label84/laravel-auth-log": "^1.4",
    "laravel/framework": "^12.0",
    "laravel/tinker": "^2.9",
    "laravel/ui": "^4.5",
    "livewire/livewire": "^3.5",
    "mpociot/teamwork": "dev-l12-compatibility",
    "rappasoft/laravel-livewire-tables": "dev-laravel12-support",
    "spatie/eloquent-sortable": "^4.4",
    "spatie/laravel-activitylog": "^4.9",
    "spatie/laravel-permission": "^6.15",
    "spatie/laravel-searchable": "^1.13",
    "victorybiz/laravel-simple-select": "dev-l12-compatibility",
    "wire-elements/modal": "^2.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^10.5",
    "spatie/laravel-ignition": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "patches": {
      "jackbayliss/livewire-select2": {
        "Livewire select2 model": "livewire-select2.patch"
      }
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/laravel-shift/teamwork.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/laravel-shift/laravel-simple-select.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/svenbw/laravel-livewire-tables.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/nicolocaruso85/livewire-select2"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "cweagans/composer-patches": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
